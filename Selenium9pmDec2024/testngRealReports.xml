<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="AllSitesSuite" parallel="false">

    <listeners>
        <listener class-name="org.testng.reporters.EmailableReporter2"/>
    </listeners>

    <test name="Social and Job Sites Tests">
        <classes>
            <class name="TestNGDemo.DemoReal.SocialSites" />
            <class name="TestNGDemo.DemoReal.JobSites" />
        </classes>
    </test>

    <test name="Tutorial and AI Sites Tests">
        <classes>
            <class name="TestNGDemo.DemoReal.TutorialSites" />
            <class name="TestNGDemo.DemoReal.AISites" />
        </classes>
    </test>
</suite>
